//16] Write a C program to find the largest element in an array of integers.
#include<stdio.h>
int main(){
	
	int i , n;
	
	printf("enter elements of string : ");
	scanf("%d",&n);
	
	int arr[n];
	
	printf("enter value of array :");
	for(i = 0; i< n; i++){
		scanf("%d",&arr[i]);
	}
	
	int largest = arr[0];
	
	for(i=1;i<n;i++){
		if(arr[i]>largest){
			largest = arr [i];
			
		}
	}
	printf("largest number of arr :%d",largest);
}